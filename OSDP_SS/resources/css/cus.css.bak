/**user nomination*/
ul.item_container {
    list-style-type: none;
    border: 1px solid;
    height: 160px;
    padding: 10px 20px;
}

li.active {
     background-color: #94aaff; 
}

.icon-go-container{
    border-radius: 4px;
    width: 40px;
    height: 40px;
    padding: 0;
    color: #5f79e0;
    font-size: 1.125em;
    text-align: center;
    line-height: 38px;
    box-shadow: 1px 1px 2px 0px rgba(0,0,0,0.1);
    position: relative;
    background-color: #fff;
    border: 1px solid #ddd;
    padding-left: 7px;
    cursor: pointer;
    margin: 20px auto;
}

ul.item_container_big {
    list-style-type: none;
    border: 1px solid;
